<script>
var xValues = ["G", "P","L","R","K","F","S","A","N","E","T","V","*","Q","W","I","D","C","Y","H","M"];
var yValues = [401646,401646,257119,184907,170365,167557,147779,122110,110282,95559,87992,87992,82065,77342,76089,42766,7660,7110,7008,6512,704];
var barColors = [];

new Chart("Chromosome 1", {
  type: "bar",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: yValues
    }]
  },
  options: {
    legend: {display: false},
    title: {
      display: true,
      text: "Chromosome 1"
    }
  }
});
</script>

