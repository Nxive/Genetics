<script class = "chart">
var xValues = ["AA", "Arg","Asn","Asp","Cys","Gln","Glu","Gly","His","Ile","Leu","Lys", "Met", "Phe", "Pro", "Ser", "Thr", "Trp", "Tyr", "Val", "Stop"];
var y1 = [29, 50, 11, 19, 13, 19, 27, 47, 14, 33, 45, 27, 1, 26, 71, 55, 36, 12, 15, 41, 13];
var barColors = [];

new Chart("Chromosome 1", {
  type: "bar",
  data: {
    labels: xValues,
    datasets: [{
      backgroundColor: barColors,
      data: y1
    }]
  },
  options: {
    legend: {display: false},
    title: {
      display: true,
      text: "Enzymes in Chromosome 1"
    }
  }
});
</script>

